 */
public class Controller {

	/**
	 * @param args
	 */
	static Model numObj;
	public static void main(String[] args) {
		 numObj = new Model(0,0);
		

	}
	
	private static void getSumAvg(Model numbers){

        for (int i=1; i < 10; i++){
            //Here the controller interfaces with 
            //the model
            Controller.numObj.sum = numbers.sum + i;
            Controller.numObj.avg = numbers.sum/i;

        }

}
}
